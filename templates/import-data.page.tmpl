{{template "admin" .}}

{{define "page-title"}}
    Import Data
{{end}}

{{define "content"}}
    <div class="col-md-12">
        Import Data content
        <form enctype="multipart/form-data" action="/admin/upload-new-products" method="post">
            <input type="file" name="uploadfile" accept="text/csv"/>
            <input type="hidden" name="token" value="{{.}}"/>
            <input type="submit" value="upload" />
        </form>
    </div>
{{end}}